
<%= form_for @album, :html => { :multipart => true } do |f| %>

  <p>
    <%= f.label :TITLE %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :DESCRIPTION %><br>
    <%= f.text_field :description %>
  </p>

 <p>
 </p>
 <p>
     <%= f.file_field :photo %>
  </p>
  <p>
    <%= f.submit %>
  </p>

<% end %>
